{
    "name": "jeffersonbehling/materialize-theme-cakephp",
    "description": "Materialize plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Jefferson Vantuir",
            "homepage": "https://jeffersonbehling.github.io",
            "role": "Author"
        }
    ],
    "config": {
        "preferred-install": "src"
    },
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Materialize\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Materialize\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
